{
  "canisters": {
    "canister1": {
      "build": "npx azle canister1",
      "candid": "src/canister1/canister1.did",
      "gzip": true,
      "main": "src/canister1/index.ts",
      "type": "custom",
      "wasm": ".azle/canister1/canister1.wasm",
      "env": ["CANISTER_ID_CANISTER2", "CANISTER_ID_CANISTER1"]
    },
    "canister2": {
      "build": "npx azle canister2",
      "candid": "src/canister2/canister2.did",
      "gzip": true,
      "main": "src/canister2/index.ts",
      "type": "custom",
      "wasm": ".azle/canister2/canister2.wasm",
      "env": ["CANISTER_ID_CANISTER2", "CANISTER_ID_CANISTER1"]
    }
  },
  "defaults": {
    "build": {
      "args": "",
      "packtool": ""
    }
  },
  "output_env_file": ".env",
  "version": 1
}
